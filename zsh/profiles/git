alias git_account_setup="git config user.name qgadrian && git config user.email qgadrian@users.noreply.github.com"

alias gs="git status"
alias gl="git log"
alias glp="git log -p"
alias gd="git diff"
alias gdc="git diff --cached"

# Peco aliases
alias gss="git stash list | peco | sed 's/: .*//g' | xargs -I{} -ot git stash show -p {}"
